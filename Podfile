  pod 'HYCategory'   ,:path=> './PrivatePods/HYCategory'
  pod 'HYWebModule'   ,:path=> './PrivatePods/HYWebModule'
  pod 'HYThirdPartyLibrary'   ,:path=> './PrivatePods/HYThirdPartyLibrary'
